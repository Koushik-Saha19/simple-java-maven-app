pipeline {
    agent any
    tools {
        maven 'LocalMVN'
        jdk 'LocalJDK'
    }
    stages {
        stage ('Jmeter Test') {
            steps {
                sh '/opt/jmeter/apache-jmeter-5.2.1/bin/jmeter.sh -n -t /home/ubuntu/test4.jmx -l /home/ubuntu/report.jtl'
        
            }
            
        }
    }
    post {
        always {
            step([$class: 'ArtifactArchiver', artifacts: '**/*.jtl'])
        }
    }
}
