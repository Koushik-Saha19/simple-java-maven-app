pipeline {
    agent any
    tools {
        maven 'LocalMVN'
        jdk 'LocalJDK'
    }
    stages {
        stage ('Jmeter Test') {
            steps {
                sh '/opt/jmeter/apache-jmeter-5.2.1/bin/jmeter.sh -t /home/ubuntu/test.jmx -n -l /home/ubuntu/report.jtl'
            }
        }
    }
}
