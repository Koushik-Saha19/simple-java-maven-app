pipeline {
    agent any
    tools {
        maven 'LocalMVN'
        jdk 'LocalJDK'
    }
    stages {
        stage ('Jmeter Test') {
            steps {
                sh 'mvn clean verify'
        
            }
            
        }
    }
    post {
        always {
            step([$class: 'ArtifactArchiver', artifacts: '**/*.jtl'])
        }
    }
}
