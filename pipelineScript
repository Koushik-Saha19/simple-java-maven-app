pipeline {
    agent any
    tools {
        maven 'LocalMVN'
        jdk 'LocalJDK'
    }
    stages {
        stage ('Jmeter Test') {
            steps {
                sh '/opt/jmeter/apache-jmeter-5.2.1/bin/jmeter.sh -jmeter.save.saveservice.output_format=xml -jmeter.save.saveservice.output_format=csv -n -t /home/ubuntu/test.jmx -l /home/ubuntu/report.jtl -e -o /home/ubuntu/report'
            }
        }
    }
}
